<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ojcity.android.myscorecards" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ojcity.android.myscorecards.Activities.MainActivity"
            android:label="@string/app_name" >

             <intent-filter>
             <action android:name="android.intent.action.MAIN" />


             <category android:name="android.intent.category.LAUNCHER" />
             </intent-filter>
        </activity>

        <activity
            android:name="com.ojcity.android.myscorecards.Activities.ScoreActivity"
            android:label="@string/title_activity_score"
            android:parentActivityName="com.ojcity.android.myscorecards.Activities.MainActivity" >

            <meta-data
             android:name="android.support.PARENT_ACTIVITY"
             android:value="com.ojcity.android.myscorecards.Activities.MainActivity" />

        </activity>


    </application>

</manifest>
